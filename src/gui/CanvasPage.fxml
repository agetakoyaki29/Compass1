<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.canvas.*?>

<BorderPane minHeight="800.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CanvasPageController">
   <center>
      <ScrollPane fx:id="cover" BorderPane.alignment="CENTER">
         <content>
            <Canvas fx:id="canvas" height="200.0" width="200.0" />
         </content>
      </ScrollPane>
   </center>
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Pane fx:id="act" prefHeight="25.0" />
         </children>
      </VBox>
   </top>
   <bottom>
      <Pane fx:id="status" prefHeight="25.0" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <Pane fx:id="properties" minWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
   <left>
      <Pane fx:id="tool" minWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button1" mnemonicParsing="false" text="Button1" />
            <Button fx:id="button2" layoutY="44.0" mnemonicParsing="false" text="Button2" />
            <Button fx:id="button3" layoutY="95.0" mnemonicParsing="false" text="Button3" />
         </children></Pane>
   </left>
</BorderPane>
